<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>LoginandRegister</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
<servlet>
<servlet-name>LoginServletInfo</servlet-name>
<servlet-class>com.LoginLogoutExample.LoginServlet</servlet-class>
</servlet> 
 
<servlet-mapping>
<servlet-name>LoginServletInfo</servlet-name>
<url-pattern>/login</url-pattern>
</servlet-mapping>


<servlet>
<servlet-name>MenuServletInfo</servlet-name>
<servlet-class>com.LoginLogoutExample.MenuServlet</servlet-class>

</servlet>  
<servlet-mapping>
<servlet-name>MenuServletInfo</servlet-name>
<url-pattern>/menu</url-pattern>

</servlet-mapping>

<servlet>
<servlet-name>LogoutServletInfo</servlet-name>
<servlet-class>com.LoginLogoutExample.LogoutServlet</servlet-class>

</servlet>  
<servlet-mapping>
<servlet-name>LogoutServletInfo</servlet-name>
<url-pattern>/logout</url-pattern>



</servlet-mapping>
  
  
  
</web-app>